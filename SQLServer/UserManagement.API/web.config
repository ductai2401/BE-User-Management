<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<location path="." inheritInChildApplications="false">
		<appSettings>
			<add key="Cors-Origins" value="http://localhost:5000" />
			<add key="Cors-Origins" value="http://localhost:4200" />
		</appSettings>
		<system.web>
			<httpRuntime maxRequestLength="2147483647" />
		</system.web>
		<system.webServer>
			<handlers>
				<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
			</handlers>
			<security>
				<requestFiltering>
					<requestLimits maxAllowedContentLength="209715200" />
				</requestFiltering>
			</security>
			<aspNetCore processPath="bin\Debug\net5.0\UserManagement.API.exe" arguments="" stdoutLogEnabled="false" hostingModel="InProcess">
				<environmentVariables>
					<environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Development" />
				</environmentVariables>
			</aspNetCore>
		</system.webServer>
	</location>
</configuration>